{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "kurokumaft:flame_porcupine",
      "spawn_category": "creature",
      "is_spawnable": true,
      "is_summonable": true,
      "properties": {
        "kurokumaft:flame_porcupine_state": {
          "type": "enum",
          "values": [ "unrolled", "rolled_up", "rolled_up_peeking", "rolled_up_relaxing", "rolled_up_unrolling" ],
          "default": "unrolled",
          "client_sync": true
        }
      }
    },
    "components": {
      
      "minecraft:type_family": {
        "family": [ "flame_porcupine", "mob", "magic_animal", "monster" ]
      },
      "minecraft:experience_reward": {
        "on_bred": "Math.Random(1,7)",
        "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/animal/flame_porcupine.json"
      },
      "minecraft:interact": {
        "interactions": [
          {
            "on_interact": {
              "filters": {
                "all_of": [
                  { "test": "is_family", "subject": "other", "value": "player" },
                  { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "shears" }
                ]
              }
            },
            "hurt_item": 16,
            "swing": true,
            "spawn_items": { "table": "loot_tables/entities/animal/flame_porcupine_shears.json" }
          }
        ]
      },
      "minecraft:physics": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement.basic": {
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_damage_blocks": true,
        "avoid_water": true
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:nameable": {
      },
      "minecraft:balloonable": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": 0.7,
        "height": 0.65
      },
      "minecraft:health": {
        "value": 24
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_water", "subject": "self"
            },
            "cause": "drowning",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                { "test": "is_family", "subject": "other", "operator": "not", "value": "aqua_jackal" },
                { "test": "is_family", "subject": "other", "operator": "not", "value": "water_buffalo" }
              ]
            }
          }
        ]
      },
      "minecraft:attack": {
        "damage": 4
      },
      "minecraft:behavior.melee_attack": {
        "priority": 3,
        "cooldown_time": 2,
        "reach_multiplier": 0.5
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "must_see": true,
        "reselect_targets": true,
        "within_radius": 10.0,
        "must_see_forget_duration": 17.0,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                { "test": "is_family", "subject": "other", "value": "monster" },
                { "test": "is_family", "subject": "other", "operator": "not", "value": "flame_porcupine" },
                { "test": "is_family", "subject": "other", "operator": "not", "value": "aqua_jackal" },
                { "test": "is_family", "subject": "other", "operator": "not", "value": "water_buffalo" }
              ]
            },
            "max_dist": 16,
            "must_see": true
          },
          {
            "filters": {
              "any_of": [
                { "test": "is_family", "subject": "other", "value": "player" },
                { "test": "is_family", "subject": "other", "value": "familiar" }
              ]
            },
            "max_dist": 16,
            "must_see": true
          },
          {
            "filters": {
              "any_of": [
                { "test": "is_family", "subject": "other", "value": "snow_wolf" },
                { "test": "is_family", "subject": "other", "value": "frost_owl" }
              ]
            },
            "max_dist": 32,
            "must_see": false
          }
        ]
      },
      "minecraft:entity_sensor": {
        "subsensors": [
          {
            "event": "minecraft:no_threat_detected",
            "cooldown": 0.2,
            "range": [ 7.0, 2.0 ],
            "minimum_count": 0,
            "maximum_count": 0,
            "event_filters": {
              "any_of": [
                { "test": "is_family", "subject": "other", "value": "undead" },
                {
                  "all_of": [
                    { "test": "is_family", "subject": "other", "value": "player" },
                    {
                      "any_of": [
                        { "test": "was_last_hurt_by", "subject": "other" },
                        { "test": "is_sprinting", "subject": "other" },
                        { "test": "is_riding", "subject": "other" }
                      ]
                    }
                  ]
                }
              ]
            }
          },
          {
            "event": "minecraft:threat_detected",
            "cooldown": 0.2,
            "range": [ 7.0, 2.0 ],
            "minimum_count": 1,
            "event_filters": {
              "any_of": [
                { "test": "is_family", "subject": "other", "value": "undead" },
                {
                  "all_of": [
                    { "test": "is_family", "subject": "other", "value": "player" },
                    {
                      "any_of": [
                        { "test": "was_last_hurt_by", "subject": "other" },
                        { "test": "is_sprinting", "subject": "other" },
                        { "test": "is_riding", "subject": "other" }
                      ]
                    }
                  ]
                }
              ]
            }
          }
        ]
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 1,
        "ignore_mob_damage": true,
        "speed_multiplier": 2.0
      }
    },

    "component_groups": {
      "minecraft:unrolled": {
        "minecraft:movement": {
          "value": 0.3
        },
        "minecraft:ambient_sound_interval": {
        },
        "minecraft:damage_sensor": {
          "triggers": {
            "on_damage": {
              "filters": {
                "any_of": [
                  { "test": "is_family", "subject": "other", "value": "mob" },
                  { "test": "is_family", "subject": "other", "value": "player" }
                ]
              },
              "event": "minecraft:threat_detected"
            }
          }
        },
        "minecraft:behavior.tempt": {
          "priority": 3,
          "speed_multiplier": 1.25,
          "can_tempt_vertically": true,
          "items": [ "spider_eye" ]
        },
        "minecraft:behavior.random_stroll": {
          "priority": 6,
          "speed_multiplier": 1.0
        },
        "minecraft:behavior.look_at_player": {
          "priority": 7,
          "target_distance": 6.0,
          "probability": 0.02,
          "min_look_time": 40,
          "max_look_time": 80
        },
        "minecraft:behavior.random_look_around": {
          "priority": 8
        }
      },
      "minecraft:rolled_up": {
        "minecraft:movement": {
          "value": 0.0
        },
        "minecraft:body_rotation_blocked": {
        },
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "on_damage": {
                "filters": {
                  "any_of": [
                    { "test": "is_family", "subject": "other", "value": "mob" },
                    { "test": "is_family", "subject": "other", "value": "player" }
                  ]
                },
                "event": "minecraft:threat_detected"
              },
              "damage_multiplier": 1.4,
              "damage_modifier": 1.0
            },
            {
              "damage_multiplier": 1.4,
              "damage_modifier": 1.0
            }
          ]
        },
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "filters": {
                "any_of": [
                  { "test": "on_fire" },
                  { "test": "in_water" },
                  { "test": "is_panicking" },
                  { "test": "is_leashed" },
                  { "test": "is_riding" }
                ]
              },
              "event": "minecraft:unroll"
            }
          ]
        }
      },
      "minecraft:rolled_up_with_threats": {
        "minecraft:behavior.timer_flag_1": {
          "priority": 0,
          "cooldown_range": 2.5,
          "duration_range": [ 5.0, 20.0 ],
          "on_start": {
            "event": "minecraft:stop_peeking"
          },
          "on_end": {
            "event": "minecraft:start_peeking"
          }
        }
      },
      "minecraft:rolled_up_without_threats": {
        "minecraft:timer": {
          "looping": true,
          "time": 4,
          "randomInterval": false,
          "time_down_event": {
            "event": "minecraft:unroll"
          }
        },
        "minecraft:behavior.timer_flag_1": {
          "priority": 0,
          "cooldown_range": 2.5,
          "duration_range": 1.5,
          "on_start": {
            "event": "minecraft:start_unrolling"
          }
        }
      }
    },

    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "minecraft:unrolled",
            "minecraft:scanning"
          ]
        }
      },
      "minecraft:no_threat_detected": {
        "sequence": [
          {
            "filters": {
              "all_of": [
                {
                  "any_of": [
                    { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up" },
                    { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up_peeking" }
                  ]
                },
                { "test": "on_fire", "value": false },
                { "test": "in_water", "value": false },
                { "test": "is_panicking", "value": false },
                { "test": "is_leashed", "value": false },
                { "test": "is_riding", "value": false }
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:rolled_up_with_threats"
              ]
            },
            "add": {
              "component_groups": [
                "minecraft:rolled_up_without_threats"
              ]
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up_relaxing"
            }
          }
        ]
      },
      "minecraft:threat_detected": {
        "sequence": [
          {
            "filters": {
              "all_of": [
                { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "unrolled" },
                { "test": "on_ground", "value": true },
                { "test": "on_fire", "value": false },
                { "test": "in_water", "value": false },
                { "test": "is_panicking", "value": false },
                { "test": "is_leashed", "value": false },
                { "test": "is_riding", "value": false }
              ]
            },
            "trigger": "minecraft:roll_up"
          },
          {
            "filters": {
              "any_of": [
                { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up_relaxing" },
                { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up_unrolling" }
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:rolled_up_without_threats"
              ]
            },
            "add": {
              "component_groups": [
                "minecraft:rolled_up_with_threats"
              ]
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up"
            }
          }
        ]
      },
      "minecraft:unroll": {
        "sequence": [
          {
            "filters": {
              "all_of": [
                { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "operator": "not", "value": "unrolled" },
                { "test": "actor_health", "subject": "self", "operator" : ">", "value": 0 }
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:rolled_up",
                "minecraft:rolled_up_with_threats",
                "minecraft:rolled_up_without_threats"
              ]
            },
            "add": {
              "component_groups": [
                "minecraft:unrolled"
              ]
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "unrolled"
            },
            "emit_vibration": {
              "vibration": "entity_act"
            }
          }
        ]
      },
      "minecraft:roll_up": {
        "sequence": [
          {
            "filters": {
              "all_of": [
                { "test": "enum_property", "domain": "minecraft:armadillo_state", "operator": "not", "value": "rolled_up" },
                { "test": "enum_property", "domain": "minecraft:armadillo_state", "operator": "not", "value": "rolled_up_peeking" },
                { "test": "actor_health", "subject": "self", "operator" : ">", "value": 0 }
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:unrolled",
                "minecraft:rolled_up_without_threats"
              ]
            },
            "add": {
              "component_groups": [
                "minecraft:rolled_up",
                "minecraft:rolled_up_with_threats"
              ]
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up"
            }
          },
          {
            "filters": {
              "all_of": [
                { "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "unrolled" },
                { "test": "actor_health", "subject": "self", "operator" : ">", "value": 0 }
              ]
            },
            "emit_vibration": {
              "vibration": "entity_act"
            }
          }
        ]
      },
      "minecraft:start_peeking": {
        "sequence": [
          {
            "filters": {
              "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up"
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up_peeking"
            }
          }
        ]
      },
      "minecraft:stop_peeking": {
        "sequence": [
          {
            "filters": {
              "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up_peeking"
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up"
            }
          }
        ]
      },
      "minecraft:start_unrolling": {
        "sequence": [
          {
            "filters": {
              "test": "enum_property", "domain": "kurokumaft:flame_porcupine_state", "value": "rolled_up_relaxing"
            },
            "set_property": {
              "kurokumaft:flame_porcupine_state": "rolled_up_unrolling"
            }
          }
        ]
      }
    }
  }
}
