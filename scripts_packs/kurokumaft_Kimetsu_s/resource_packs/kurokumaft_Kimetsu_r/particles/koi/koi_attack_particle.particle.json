{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "kurokumaft:koi_attack_particle",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/particles"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": "query.property('kurokumaft:kaikyu')*30",
				"max_particles": "query.property('kurokumaft:kaikyu')*30"
			},
			"minecraft:emitter_lifetime_expression": {
				"activation_expression": 1,
				"expiration_expression": "(variable.emitter_age > 0.5 ? 1.0 : 0.0)"
			},
			"minecraft:emitter_shape_box": {
				"offset": ["math.random_integer(-0.1, 0.1)", "math.random_integer(-0.1, 0.1)", "math.random_integer(-0.1, 0.1)"],
				"half_dimensions": [0.1, 0.4, 0.1],
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.2
			},
			"minecraft:particle_initial_speed": 0.2,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": [0.25, 0.25],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"uv": [16, 32],
					"uv_size": [7, 7]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.18": "#FFB24397",
						"1.0": "#FFE6B2DC"
					}
				}
			}
		}
	}
}
