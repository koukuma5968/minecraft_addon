{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "kurokumaft:hi_attack_particle",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/particles"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": "query.property('kurokumaft:kaikyu')*10",
				"max_particles": "query.property('kurokumaft:kaikyu')*20"
			},
			"minecraft:emitter_lifetime_expression": {
				"activation_expression": 1,
				"expiration_expression": "(variable.emitter_age > 0.25 ? 1.0 : 0.0)"
			},
			"minecraft:emitter_shape_box": {
				"offset": [0, -0.25, 0],
				"half_dimensions": [0.1, 0.75, 0.1]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.3
			},
			"minecraft:particle_initial_speed": 0,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": [0.15, 0.15],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"flipbook": {
						"base_UV": [0, 24],
						"size_UV": [8, 8],
						"step_UV": [5, 80],
						"frames_per_second": 10,
						"max_frame": 10
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.14": "#FFC97070",
						"0.86": "#FF6C1010"
					}
				}
			}
		}
	}
}
