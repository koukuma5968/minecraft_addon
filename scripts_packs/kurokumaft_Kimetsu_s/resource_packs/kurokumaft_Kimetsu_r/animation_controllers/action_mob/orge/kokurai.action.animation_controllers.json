{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.kokurai.action" : {
			"initial_state" : "default",
			"states" : {
				"default":{
					"transitions" : [
						{"third_use": "query.property('kurokumaft:kokyu_use')"}
					]
				},
				"third_use": {
					"transitions" : [
						{"kokurai2_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 2"},
						{"kokurai3_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 3"},
						{"kokurai4_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 4"},
						{"kokurai5_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 5"},
						{"kokurai6_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 6"}
					]
				},
				"kokurai2_1_third" : {
					"animations" : [
						"kokurai.ni_1_third"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"kokurai3_1_third" : {
					"animations" : [
						"kokurai.san_1_third"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"kokurai4_1_third" : {
					"animations" : [
						"kokurai.shi_1_third"
					],
					"transitions" : [
						{"kokurai4_2_third":"!query.property('kurokumaft:kokyu_use')"}
					]
				},
				"kokurai4_2_third" : {
					"animations" : [
						"kokurai.shi_2_third"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"kokurai5_1_third" : {
					"animations" : [
						"kokurai.go_1_third"
					],
					"transitions" : [
						{"default":"!query.property('kurokumaft:kokyu_use')"}
					]
				},
				"kokurai6_1_third" : {
					"animations" : [
						"kokurai.roku_1_third"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				}

			}
		}
	}
}