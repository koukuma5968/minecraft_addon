				"default":{
					"transitions" : [
						{"third_use": "query.property('kurokumaft:kokyu_use')"}
					]
				},
				"third_use": {
					"transitions" : [
						{"mikazuki1_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 1"},
						{"mikazuki2_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 2"},
						{"mikazuki3_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 3"},
						{"mikazuki5_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 5"},
						{"mikazuki6_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 6"},
						{"mikazuki7_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 7"},
						{"mikazuki8_1_third": "query.property('kurokumaft:kekkizyutu_kata') == 8"},
