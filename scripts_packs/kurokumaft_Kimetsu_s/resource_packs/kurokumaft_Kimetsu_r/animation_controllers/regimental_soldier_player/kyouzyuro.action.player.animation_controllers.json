				},
				"hono4_third" : {
					"animations" : [
						"hono.shi_1_third"
					],
					"transitions" : [
						{"equipment":"query.any_animation_finished"}
					]
				},
				"hono5_third" : {
					"animations" : [
						"hono.go_1_third"
					],
					"transitions" : [
						{"equipment":"query.any_animation_finished"}
					]
				},
				"hono9_third" : {
					"animations" : [
						"hono.ku_1_third"
					],
					"transitions" : [
						{"hono9_2_third": "!query.property('kurokumaft:kokyu_use')"}
					]
				},
				"hono9_2_third" : {
					"animations" : [
						"hono.ku_2_third"
					],
					"transitions" : [
						{"equipment": "query.any_animation_finished"}
					]
				}

			}
		}
	}
}